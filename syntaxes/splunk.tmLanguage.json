{
	"fileTypes": [
	  "conf",
	  "conf.example",
	  "conf.default",
	  "conf.spec",
	  "conf.in",
	  "conf.example.in",
	  "conf.spec.in",
	  "cfg.spec.in"
	],
	"name": "splunk_conf",
	"patterns": [
		{
			"include": "#keywords"
		},
		{
			"include": "#strings"
		},
	  {
		"comment": "default conf file warning",
		"match": "^# DO NOT EDIT THIS FILE\\!$",
		"name": "invalid.illegal"
	  },
	  {
		"begin": "(^[ \\t]+)?(?=#)",
		"beginCaptures": {
		  "1": {
			"name": "comment.line.number-sign.splunk_conf"
		  }
		},
		"end": "(?!\\G)",
		"patterns": [
		  {
			"begin": "#",
			"beginCaptures": {
			  "0": {
				"name": "comment.line.number-sign.splunk_conf"
			  }
			},
			"end": "\\n",
			"name": "comment.line.number-sign.splunk_conf"
		  }
		]
	  },
	  {
		"begin": "(^[ \\t]+)?(?=;)",
		"beginCaptures": {
		  "1": {
			"name": "comment.line.splunk_conf"
		  }
		},
		"end": "(?!\\G)",
		"patterns": [
		  {
			"begin": ";",
			"beginCaptures": {
			  "0": {
				"name": "comment.line.semicolon.splunk_conf"
			  }
			},
			"end": "\\n",
			"name": "comment.line.semicolon.splunk_conf"
		  }
		]
	  },
	  {
		"captures": {
		  "1": {
			"name": "keyword.other.definition.splunk_conf"
		  },
		  "2": {
			"name": "punctuation.separator.key-value.splunk_conf"
		  },
		  "3": {
			"name": "variable.language.splunk_conf"
		  }
		},
		"match": "\\b([a-zA-Z0-9_.-]+)\\b\\s*(=)\\s*(.*)$"
	  },
	  {
		"captures": {
		  "1": {
			"name": "punctuation.definition.entity.splunk_conf"
		  },
		  "3": {
			"name": "punctuation.definition.entity.splunk_conf"
		  }
		},
		"match": "^(\\[)(.*?)(\\])",
		"name": "entity.name.section.group-title.splunk_conf"
	  },
	  {
		"begin": "'",
		"beginCaptures": {
		  "0": {
			"name": "punctuation.definition.string.begin.splunk_conf"
		  }
		},
		"end": "'",
		"endCaptures": {
		  "0": {
			"name": "punctuation.definition.string.end.splunk_conf"
		  }
		},
		"name": "string.quoted.single.splunk_conf",
		"patterns": [
		  {
			"match": "\\\\.",
			"name": "constant.character.escape.splunk_conf"
		  }
		]
	  },
	  {
		"begin": "\"",
		"beginCaptures": {
		  "0": {
			"name": "punctuation.definition.string.begin.splunk_conf"
		  }
		},
		"end": "\"",
		"endCaptures": {
		  "0": {
			"name": "punctuation.definition.string.end.splunk_conf"
		  }
		},
		"name": "string.quoted.double.splunk_conf"
	  }
	],
	"scopeName": "source.splunk_conf"
  }